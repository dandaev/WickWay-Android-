<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <TextView
        android:padding="20dp"
        android:id="@+id/aboutUsTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/aboutUs"
        android:textSize="20sp"
        android:textColor="#000"/>

    <ImageView
        android:id="@+id/whzImageView"
        android:padding="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/aboutUsTextView"
        android:src="@drawable/whz"/>

    <ImageView
        android:id="@+id/inaiImageView"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_below="@id/whzImageView"
        android:layout_marginLeft="3dp"
        android:layout_marginBottom="1dp"
        android:padding="5dp"
        android:src="@drawable/inai" />

</RelativeLayout>